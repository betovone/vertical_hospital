<?xml version="1.0"?>
<odoo>
	<template id="vertical_hospital_paciente_template">
		<t t-call="web.html_container">
		    <t t-foreach="docs" t-as="o">
		        <t t-call="web.external_layout">
		            <div class="page">
		                <div style="font-size:36px; text-align: center"><t t-esc="o.nombre_apellido" /></div>
						<div>Secuencia: <t t-esc="o.name" /></div>
						<div>DNI: <t t-esc="o.dni" /></div>
						<div>Estado: <t t-esc="o.state" /></div>
		            </div>
		        </t>
		    </t>
		</t>
	</template>


</odoo>